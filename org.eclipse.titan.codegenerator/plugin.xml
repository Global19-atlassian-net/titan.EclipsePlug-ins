<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
         <actionSet
           id="org.eclipse.titan.designer.actionSet1"
           label="GenerateJAVAfiles"
           visible="true">
        <action
              class="org.eclipse.titan.codegenerator.AstWalkerJava"
              icon="icons/generate_java.gif"
              id="org.eclipse.titan.designer.action1"
              label="GenerateJAVAfiles"
              state="true"
              style="push"
              toolbarPath="Normal/additions"
              tooltip="Generate JAVA code from the TTCN sources">
        </action>
        <action
              class="org.eclipse.titan.codegenerator.AstWalkerRunnerJava"
              icon="icons/generate_java_run.gif"
              id="org.eclipse.titan.designer.action2"
              label="GenerateJAVAfilesRun"
              state="true"
              style="push"
              toolbarPath="Normal/additions"
              tooltip="Generate JAVA code from the TTCN sources and run">
        </action>
        <action
              class="org.eclipse.titan.codegenerator.AstRunnerJava"
              icon="icons/run_java.gif"
              id="org.eclipse.titan.designer.action3"
              label="RunJAVAfiles"
              state="true"
              style="push"
              toolbarPath="Normal/additions"
              tooltip="Run JAVA code">
        </action>
     </actionSet>
   </extension>
</plugin>